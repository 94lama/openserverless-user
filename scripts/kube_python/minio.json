{
  "apiVersion": "v1",
  "kind": "Pod",
  "metadata": {
    "name": "minio-pod",
    "labels": {
      "app": "webserver"
    }
  },
  "spec": {
    "containers": [
      {
        "name": "minio-container",
        "image": "minio",
        "ports": [
          {
            "containerPort": 9000
          }
        ]
      }
    ]
  }
}